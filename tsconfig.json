{
    "compilerOptions": {
      "module": "es6",
      "moduleResolution": "node",
      "noImplicitReturns": true,
      "outDir": "./dist/",
      "strict": true,
      "lib": [ "dom", "es6", "es2015.collection" ],
      "target": "es5",
      "jsx": "react",
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true
    },
    "include": [
      "src"
    ],
    "exclude": [
      "node_modules",
      "src/components/__tests__"
    ]
  }